<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" />
<Script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.min.js"></Script>

<div id="main">
	<div class="h-tag">
		<h2>WFM User Login Resource Management</h2>
	</div>
	<!-- Login box -->
	<div class="login">
	<table cellspacing="2" align="center" cellpadding="8" border="0">
	<tr>
	<td>User Name :</td>
	<td><input type="text" placeholder="Enter User Name " id="uname" /></td>
	</tr>
	<tr>
	<td>Password :</td>
	<td><input type="password" placeholder="Enter Password" id="psw"/></td>
	</tr>
	<tr>
	<td></td>
	<td>
	<input type="submit" value="Reset" onclick="clearFunc()" class="btn" />
	<input type="submit" value="Login" class="btn" id="submit"/></td>
	</tr>
	</table>
	</div>
  	 <!-- login box div ending here.. -->
	</div>
	<!-- Main div ending here... -->

<script src="~/js//Login.js"></script>
